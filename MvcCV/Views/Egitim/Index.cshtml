@using MvcCV.Models.Entity
@model List<TblEğitimler>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Eğitim Listesi</h2>
<table class="table table-secondary">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Altbaşlık1</th>
        <th>AltBaşlık 2</th>
        <th>GNO</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {

        <tr>
            <td>@item.ID</td>
            <td>@item.Başlık</td>
            <td>@item.AltBaşlık1</td>
            <td>@item.AltBaşlık2</td>
            <td>@item.GNO</td>
            <td>@item.Tarih</td>
            <td><a href="/Egitim/EgitimSil/@item.ID" class="btn btn-warning">Sil</a></td>
            <td><a href="/Egitim/EgitimDüzenle/@item.ID" class="btn btn-warning">Düzenle</a></td>
        </tr>
    }
</table>
<a href="/Egitim/EgitimEkle/" class="btn btn-success">Yeni Eğitim Ekle</a>


