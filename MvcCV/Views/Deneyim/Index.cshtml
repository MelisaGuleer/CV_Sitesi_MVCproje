@using MvcCV.Models.Entity
@model List<TblDeneyimler>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br/>
<h2>Deneyim Listesi</h2>
<table class="table table-danger">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Altbaşlık</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {

        <tr>
            <td>@item.ID</td>
            <td>@item.Başlık</td>
            <td>@item.Altbaşlık</td>
            <td>@item.Tarih</td>
            <td><a href="/Deneyim/DeneyimSil/@item.ID"  class="btn btn-warning">Sil</a></td>
            <td><a href="/Deneyim/DeneyimGetir/@item.ID"  class="btn btn-warning">Düzenle</a></td>
        </tr>
    }
</table>
<a href="/Deneyim/DeneyimEkle/" class="btn btn-dark">Yeni Deneyim Ekle</a>

