@using MvcCV.Models.Entity
@model List<TblAdmin>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Admin İşlemleri</h2>
<table class="table table-danger">
    <tr>
        <th>ID</th>
        <th>Kullanıcı Adı</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {

        <tr>
            <td>@item.ID</td>
            <td>@item.KullanıciAdı </td>
            <td><a href="/Admin/AdminSil/@item.ID" class="btn btn-warning">Sil</a></td>
            <td><a href="/Admin/AdminDuzenle/@item.ID" class="btn btn-warning">Düzenle</a></td>
        </tr>
    }
</table>
<a href="/Admin/AdminEkle/" class="btn btn-dark">Yeni Deneyim Ekle</a>



